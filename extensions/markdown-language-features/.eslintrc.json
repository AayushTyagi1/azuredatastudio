{
	"rules": {
		"@typescript-eslint/naming-convention": [
			"warn",
			{
				"selector": "default",
				"modifiers": ["private"],
				"format": null,
				"leadingUnderscore": "require"
			},
			{
				"selector": "default",
				"modifiers": ["public"],
				"format": null,
				"leadingUnderscore": "forbid"
			}
		],
		"@typescript-eslint/await-thenable": ["off"],
		"@typescript-eslint/no-unsafe-assignment": "off"
	}
}
