{
	"name": "database-documentation",
	"displayName": "%displayName%",
	"description": "%description%",
	"publisher": "Microsoft",
	"version": "0.0.1",
	"license": "https://raw.githubusercontent.com/Microsoft/azuredatastudio/main/LICENSE.txt",
	"engines": {
		"vscode": "^1.25.0",
		"azdata": ">=1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "database-documentation.generateDocumentation",
				"title": "%database-documentation.start%"
			},
			{
				"command": "database-documentation.documentTableADS",
				"title": "%database-documentation.start%"
			},
			{
				"command": "database-documentation.documentDatabaseCode",
				"title": "%database-documentation.start%"
			},
			{
				"command": "database-documentation.documentTableCode",
				"title": "%database-documentation.start%"
			}
		],
		"menus": {
			"objectExplorer/item/context": [
				{
					"command": "database-documentation.generateDocumentation",
					"when": "connectionProvider == MSSQL && nodeType && nodeType =~ /^(Database|Schema|Table)$/ && mssql:engineedition != 11",
					"group": "export"
				}
			],
			"view/item/context": [
				{
					"command": "database-documentation.documentDatabaseCode",
					"when": "view == objectExplorer && viewItem =~ /^(Database)$/",
					"group": "export"
				},
				{
					"command": "database-documentation.documentTableCode",
					"when": "view == objectExplorer && viewItem =~ /^(Table)$/",
					"group": "export"
				}
			]
		},
		"configuration": {
			"openAI.apiKey": {
                "type": "string",
                "default": "",
                "description": "API key for OpenAI"
            }
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"azdata-test": "^1.x",
		"eslint": "^6.8.0",
		"glob": "^7.1.7",
		"mermaid": "^10.1.0",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5",
		"vscode-nls": "^4.0.0",
		"vscode-languageclient": "5.2.1"
	},
	"dependencies": {
		"@microsoft/ads-extension-telemetry": "^3.0.1",
		"azdata": "^1.0.0",
		"openai": "^3.2.1",
		"vscode-nls": "^4.0.0",
		"vscode-languageclient": "5.2.1"
	}
}
